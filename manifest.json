{

    "manifest_version": 2,
    "name": "SpeeedDial",
    "version": "1.0",

    "description": "Add the ability to speed dial a link",


    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["libs/jquery-3.2.1.min.js", "content_scripts/content_script.js"],
        "css": ["content_scripts/content_style.css"]
    }],
    "icons": {
        "48": "img/icon-48.png"

    },
    "background": {
        "scripts": ["background_scripts/background.js"]
    },
    "browser_action": {
        "default_icon": {
            "64": "img/icon-64.png",
            "32": "img/icon-32.png",
            "16": "img/icon-16.png"
        },
        "default_title": "OpenPage"
    },
    "options_ui": {
        "page": "options/options.html"
    },
    "permissions": ["storage", "tabs"],
    "commands": {
        "ctrl+q": {
            "suggested_key": {
                "default": "Ctrl+Q"
            }
        },
        "alt+shift+q": {
            "suggested_key": {
                "default": "Alt+Shift+Q"
            }
        }
    }
}