{

    "manifest_version": 2,
    "name": "SpeeedDial",
    "version": "1.0",

    "description": "Add the ability to speed dial a link",


    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["libs/jquery-3.2.1.min.js", "content_scripts/content_script.js"],
        "css": ["content_scripts/content_style.css"]
    }],
    "icons": {
        "48": "icons/border-48.png"
    },
    "background": {
        "scripts": ["background_scripts/background.js"]
    },
    "options_ui": {
        "page": "options/options.html"
    },
    "permissions": ["storage"],
    "commands": {
        "ctrl+q": {
            "suggested_key": {
                "default": "Ctrl+Q"
            }
        },
        "ctrl+0": {
            "suggested_key": {
                "default": "Alt+0"
            }
        },
        "ctrl+1": {
            "suggested_key": {
                "default": "Alt+1"
            }
        },
        "ctrl+2": {
            "suggested_key": {
                "default": "Alt+2"
            }
        },
        "ctrl+3": {
            "suggested_key": {
                "default": "Alt+3"
            }
        },
        "ctrl+4": {
            "suggested_key": {
                "default": "Alt+4"
            }
        },
        "ctrl+5": {
            "suggested_key": {
                "default": "Alt+5"
            }
        },
        "ctrl+6": {
            "suggested_key": {
                "default": "Alt+6"
            }
        },
        "ctrl+7": {
            "suggested_key": {
                "default": "Alt+7"
            }
        },
        "ctrl+8": {
            "suggested_key": {
                "default": "Alt+8"
            }
        },
        "ctrl+9": {
            "suggested_key": {
                "default": "Alt+9"
            }
        }
    }
}